{
  "name": "sphring/sphring-plugin-skeleton",
  "description": "This a skeleton to make a sphring plugin",
  "type": "project",
  "require": {
    "sphring/sphring": "~0.11"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "monolog/monolog": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Arthur Halet",
      "email": "arthurh.halet@gmail.com"
    }
  ],
  "extra": {
    "sphring": {
      "extend": {
        "beanProperty": [],
        "annotationClass": [],
        "beanType": [],
        "sphringGlobal": [],
        "annotationMethodCallAfter": [],
        "annotationMethodCallBefore": [],
        "annotationClassInstantiate": []
      },
      "isPlugin": true
    }
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  }
}
